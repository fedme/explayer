<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Explayer.Views.MainPage"
             Title="Welcome to Explayer">
    
    <ContentPage.Content>
        <StackLayout>

            <Button Text="Install a new app"
                    Clicked="OnAppInstallButtonClicked" />
            
            <Button Text="Launch an app"
                    Clicked="OnAppLaunchButtonClicked" />
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>